A;AACA,UAAA,KAAA,QAAA,KAAA,MAAA,EAAA;AACA,eAAA,SAAA;AACA,OAFA,MAEA,IAAA,KAAA,QAAA,KAAA,OAAA,EAAA;AACA,eAAA,MAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA,QAAA;AACA;AACA,KAnBA;AAoBA,IAAA,KApBA,mBAoBA;AACA,aAAA,KAAA,MAAA,CAAA,IAAA,CAAA,KAAA,IAAA,MAAA;AACA,KAtBA;AAuBA,IAAA,OAvBA,qBAuBA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA;AACA;AAzBA,IAvBA;AAkDA,EAAA,OAlDA,qBAkDA;AAAA;;AACA,QAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAAA;;AACA,QAAA,WAAA,GAAA,IAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KAJA,MAIA,IAAA,WAAA,IAAA,IAAA,IAAA,WAAA,GAAA,IAAA,EAAA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KAJA,MAIA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,UAAA,GAAA,KAAA;AACA;;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,UAAA,EAAA,KAAA,UAAA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,QAAA,EAAA,KAAA,QAAA;AACA,SAAA,IAAA,CAAA,EAAA,CAAA,QAAA,EAAA,KAAA,MAAA;AACA,SAAA,IAAA,CAAA,EAAA,CAAA,aAAA,EAAA,YAAA;AACA,MAAA,KAAA,CAAA,WAAA,GAAA,IAAA;AACA,KAFA;AAGA,SAAA,IAAA,CAAA,EAAA,CAAA,cAAA,EAAA,YAAA;AACA,MAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,KAFA;;AAGA,IAAA,MAAA,CAAA,QAAA,GAAA,YAAA;AACA,aAAA,YAAA;AACA,YAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAAA;;AACA,YAAA,WAAA,GAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,UAAA,GAAA,IAAA;AACA,SAJA,MAIA,IAAA,WAAA,IAAA,IAAA,IAAA,WAAA,GAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,UAAA,GAAA,IAAA;AACA,SAJA,MAIA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,UAAA,GAAA,KAAA;AACA;;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UAAA,EAAA,KAAA,CAAA,UAAA;;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,QAAA,EAAA,KAAA,CAAA,QAAA;AACA,OAjBA,EAAA;AAkBA,KAnBA;AAoBA,GA9FA;AA+FA,EAAA,OAAA,kCACA,UAAA,CAAA,MAAA,EAAA,CAAA,UAAA,CAAA,CADA;AAEA,IAAA,MAFA,oBAEA;AAAA;;AACA,WAAA,UAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,IAAA;AACA,OAFA;AAGA,KAPA;AAQA,IAAA,aARA,2BAQA;AACA,WAAA,UAAA,GAAA,CAAA,KAAA,UAAA;AACA,WAAA,OAAA,GAAA,CAAA,KAAA,UAAA;AACA,WAAA,UAAA,GAAA,CAAA,KAAA,UAAA;AACA,WAAA,IAAA,CAAA,IAAA,CAAA,UAAA,EAAA,KAAA,UAAA;AACA,KAbA;AAcA,IAAA,QAdA,sBAcA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAhBA;AAiBA,IAAA,YAjBA,0BAiBA;AACA,WAAA,UAAA,GAAA,CAAA,KAAA,UAAA;AACA,WAAA,OAAA,GAAA,CAAA,CAAA,KAAA,UAAA;AACA,WAAA,aAAA;AACA;AArBA;AA/FA,CAAA","sourcesContent":["<template>\n  <el-container class=\"layout-cont\">\n    <el-container :class=\"[isSider?'openside':'hideside',isMobile ? 'mobile': '']\">\n      <el-row :class=\"[isShadowBg?'shadowBg':'']\" @click.native=\"changeShadow()\" />\n      <el-aside class=\"main-cont main-left\">\n        <div class=\"tilte\" :style=\"{background: backgroundColor}\">\n          <img alt class=\"logoimg\" :src=\"$GIN_VUE_ADMIN.appLogo\">\n          <h2 v-if=\"isSider\" class=\"tit-text\" :style=\"{color:textColor}\">{{ $GIN_VUE_ADMIN.appName }}</h2>\n        </div>\n        <Aside class=\"aside\" />\n      </el-aside>\n      <!-- 分块滑动功能 -->\n      <el-main class=\"main-cont main-right\">\n        <transition :duration=\"{ enter: 800, leave: 100 }\" mode=\"out-in\" name=\"el-fade-in-linear\">\n          <div\n            :style=\"{width: `calc(100% - ${isMobile?'0px':isCollapse?'54px':'220px'})`}\"\n            class=\"topfix\"\n          >\n            <el-row>\n              <!-- :xs=\"8\" :sm=\"6\" :md=\"4\" :lg=\"3\" :xl=\"1\" -->\n              <el-header class=\"header-cont\">\n                <el-col :xs=\"2\" :lg=\"1\" :md=\"1\" :sm=\"1\" :xl=\"1\">\n                  <div class=\"menu-total\" @click=\"totalCollapse\">\n                    <i v-if=\"isCollapse\" class=\"el-icon-s-unfold\" />\n                    <i v-else class=\"el-icon-s-fold\" />\n                  </div>\n                </el-col>\n                <el-col :xs=\"10\" :lg=\"14\" :md=\"14\" :sm=\"9\" :xl=\"14\">\n                  <el-breadcrumb class=\"breadcrumb\" separator-class=\"el-icon-arrow-right\">\n                    <el-breadcrumb-item\n                      v-for=\"item in matched.slice(1,matched.length)\"\n                      :key=\"item.path\"\n                    >{{ item.meta.title }}</el-breadcrumb-item>\n                  </el-breadcrumb>\n                </el-col>\n                <el-col :xs=\"12\" :lg=\"9\" :md=\"9\" :sm=\"14\" :xl=\"9\">\n                  <div class=\"fl-right right-box\">\n                    <Search />\n                    <Screenfull class=\"screenfull\" :style=\"{cursor:'pointer'}\" />\n                    <el-dropdown>\n                      <span class=\"header-avatar\" style=\"cursor: pointer\">\n                        <CustomPic />\n                        <span style=\"margin-left: 5px\">{{ userInfo.nickName }}</span>\n                        <i class=\"el-icon-arrow-down\" />\n                      </span>\n                      <el-dropdown-menu slot=\"dropdown\" class=\"dropdown-group\">\n                        <el-dropdown-item>\n                          <span>\n                            更多信息\n                            <el-badge is-dot />\n                          </span>\n                        </el-dropdown-item>\n                        <el-dropdown-item icon=\"el-icon-s-custom\" @click.native=\"toPerson\">个人信息</el-dropdown-item>\n                        <el-dropdown-item icon=\"el-icon-table-lamp\" @click.native=\"LoginOut\">登 出</el-dropdown-item>\n                      </el-dropdown-menu>\n                    </el-dropdown>\n                  </div>\n                </el-col>\n\n              </el-header>\n            </el-row>\n            <!-- 当前面包屑用路由自动生成可根据需求修改 -->\n            <!--\n            :to=\"{ path: item.path }\" 暂时注释不用-->\n            <HistoryComponent />\n          </div>\n        </transition>\n        <transition mode=\"out-in\" name=\"el-fade-in-linear\">\n          <keep-alive>\n            <router-view v-if=\"$route.meta.keepAlive && reloadFlag\" v-loading=\"loadingFlag\" element-loading-text=\"正在加载中\" class=\"admin-box\" />\n          </keep-alive>\n        </transition>\n        <transition mode=\"out-in\" name=\"el-fade-in-linear\">\n          <router-view v-if=\"!$route.meta.keepAlive && reloadFlag\" v-loading=\"loadingFlag\" element-loading-text=\"正在加载中\" class=\"admin-box\" />\n        </transition>\n        <BottomInfo />\n        <setting />\n      </el-main>\n    </el-container>\n\n  </el-container>\n</template>\n\n<script>\nimport Aside from '@/view/layout/aside'\nimport HistoryComponent from '@/view/layout/aside/historyComponent/history'\nimport Screenfull from '@/view/layout/screenfull'\nimport Search from '@/view/layout/search/search'\nimport BottomInfo from '@/view/layout/bottomInfo/bottomInfo'\nimport { mapGetters, mapActions } from 'vuex'\nimport CustomPic from '@/components/customPic'\nimport Setting from './setting'\nexport default {\n  name: 'Layout',\n  components: {\n    Aside,\n    HistoryComponent,\n    Screenfull,\n    Search,\n    BottomInfo,\n    CustomPic,\n    Setting\n  },\n  data() {\n    return {\n      show: false,\n      isCollapse: false,\n      isSider: true,\n      isMobile: false,\n      isShadowBg: false,\n      loadingFlag: false,\n      reloadFlag: true,\n      value: ''\n    }\n  },\n  computed: {\n    ...mapGetters('user', ['userInfo', 'sideMode', 'baseColor']),\n    textColor() {\n      if (this.$store.getters['user/sideMode'] === 'dark') {\n        return '#fff'\n      } else if (this.$store.getters['user/sideMode'] === 'light') {\n        return '#191a23'\n      } else {\n        return this.baseColor\n      }\n    },\n    backgroundColor() {\n      if (this.sideMode === 'dark') {\n        return '#191a23'\n      } else if (this.sideMode === 'light') {\n        return '#fff'\n      } else {\n        return this.sideMode\n      }\n    },\n    title() {\n      return this.$route.meta.title || '当前页面'\n    },\n    matched() {\n      return this.$route.matched\n    }\n  },\n  mounted() {\n    const screenWidth = document.body.clientWidth\n    if (screenWidth < 1000) {\n      this.isMobile = true\n      this.isSider = false\n      this.isCollapse = true\n    } else if (screenWidth >= 1000 && screenWidth < 1200) {\n      this.isMobile = false\n      this.isSider = false\n      this.isCollapse = true\n    } else {\n      this.isMobile = false\n      this.isSider = true\n      this.isCollapse = false\n    }\n    this.$bus.emit('collapse', this.isCollapse)\n    this.$bus.emit('mobile', this.isMobile)\n    this.$bus.on('reload', this.reload)\n    this.$bus.on('showLoading', () => {\n      this.loadingFlag = true\n    })\n    this.$bus.on('closeLoading', () => {\n      this.loadingFlag = false\n    })\n    window.onresize = () => {\n      return (() => {\n        const screenWidth = document.body.clientWidth\n        if (screenWidth < 1000) {\n          this.isMobile = true\n          this.isSider = false\n          this.isCollapse = true\n        } else if (screenWidth >= 1000 && screenWidth < 1200) {\n          this.isMobile = false\n          this.isSider = false\n          this.isCollapse = true\n        } else {\n          this.isMobile = false\n          this.isSider = true\n          this.isCollapse = false\n        }\n        this.$bus.emit('collapse', this.isCollapse)\n        this.$bus.emit('mobile', this.isMobile)\n      })()\n    }\n  },\n  methods: {\n    ...mapActions('user', ['LoginOut']),\n    reload() {\n      this.reloadFlag = false\n      this.$nextTick(() => {\n        this.reloadFlag = true\n      })\n    },\n    totalCollapse() {\n      this.isCollapse = !this.isCollapse\n      this.isSider = !this.isCollapse\n      this.isShadowBg = !this.isCollapse\n      this.$bus.emit('collapse', this.isCollapse)\n    },\n    toPerson() {\n      this.$router.push({ name: 'person' })\n    },\n    changeShadow() {\n      this.isShadowBg = !this.isShadowBg\n      this.isSider = !!this.isCollapse\n      this.totalCollapse()\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n@import '~@/style/mobile.scss';\n\n.dark{\n  background-color: #191a23 !important;\n  color: #fff !important;\n}\n.light{\n  background-color: #fff !important;\n  color: #000 !important;\n}\n</style>\n"],"sourceRoot":"src/view/layout"}]}