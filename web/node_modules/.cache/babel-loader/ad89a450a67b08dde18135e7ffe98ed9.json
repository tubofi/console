;AAAA;AACA,gBAAA,KAAA,CAAA,QAAA,CAAA,iBAAA,EAAA,IAAA,EAAA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAAA,EAKA,IALA,uEAKA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCACA,WAAA,CAAA,GAAA,CADA;;AAAA;AACA,0BAAA,GADA;;AAEA,8BAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,4BAAA,KAAA,CAAA,QAAA,CAAA;AACA,8BAAA,IAAA,EAAA,SADA;AAEA,8BAAA,OAAA,EAAA;AAFA,6BAAA;;AAIA,gCAAA,KAAA,CAAA,SAAA,CAAA,MAAA,KAAA,CAAA,IAAA,KAAA,CAAA,IAAA,GAAA,CAAA,EAAA;AACA,8BAAA,KAAA,CAAA,IAAA;AACA;;AACA,4BAAA,KAAA,CAAA,YAAA;AACA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iBALA,IAkBA,KAlBA,CAkBA,YAAA;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA;AACA,oBAAA,IAAA,EAAA,MADA;AAEA,oBAAA,OAAA,EAAA;AAFA,mBAAA;AAIA,iBAvBA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyBA,KA1BA;AA2BA,IAAA,SA3BA,qBA2BA,IA3BA,EA2BA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,IAAA,KAAA,YAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,IAAA,KAAA,WAAA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA,GAAA,CAAA;;AACA,UAAA,CAAA,KAAA,IAAA,CAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,uBAAA;AACA,aAAA,iBAAA,GAAA,KAAA;AACA;;AACA,UAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,mBAAA;AACA,aAAA,iBAAA,GAAA,KAAA;AACA;;AACA,aAAA,CAAA,KAAA,IAAA,KAAA,KAAA,MAAA;AACA,KAzCA;AA0CA,IAAA,aA1CA,yBA0CA,GA1CA,EA0CA;AACA,WAAA,iBAAA,GAAA,KAAA;;AACA,UAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;;AAIA,YAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,eAAA,YAAA;AACA;AACA,OARA,MAQA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA,GAAA,CAAA;AAFA,SAAA;AAIA;AACA,KA1DA;AA2DA,IAAA,WA3DA,yBA2DA;AACA,WAAA,QAAA,CAAA;AACA,QAAA,IAAA,EAAA,OADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA;AAIA,WAAA,iBAAA,GAAA,KAAA;AACA,KAjEA;AAkEA,IAAA,YAlEA,wBAkEA,GAlEA,EAkEA;AACA,MAAA,aAAA,CAAA,GAAA,CAAA,GAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AACA;AApEA;AAhCA,CAAA","sourcesContent":["<template>\n  <div v-loading.fullscreen.lock=\"fullscreenLoading\">\n    <div class=\"upload\">\n      <el-row>\n        <el-col :span=\"12\">\n          <el-upload\n            :action=\"`${path}/fileUploadAndDownload/upload`\"\n            :before-upload=\"checkFile\"\n            :headers=\"{ 'x-token': token }\"\n            :on-error=\"uploadError\"\n            :on-success=\"uploadSuccess\"\n            :show-file-list=\"false\"\n          >\n            <el-button size=\"small\" type=\"primary\">点击上传</el-button>\n            <div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过500kb</div>\n          </el-upload>\n        </el-col>\n        <el-col :span=\"12\">\n          带压缩的上传, (512(k)为压缩限制)\n          <upload-image v-model=\"imageUrl\" :file-size=\"512\" :max-w-h=\"1080\" @on-success=\"getTableData\" />\n          已上传文件 {{ imageUrl }}\n        </el-col>\n      </el-row>\n\n      <el-table :data=\"tableData\" border stripe>\n        <el-table-column label=\"预览\" width=\"100\">\n          <template slot-scope=\"scope\">\n            <CustomPic pic-type=\"file\" :pic-src=\"scope.row.url\" />\n          </template>\n        </el-table-column>\n        <el-table-column label=\"日期\" prop=\"UpdatedAt\" width=\"180\">\n          <template slot-scope=\"scope\">\n            <div>{{ scope.row.UpdatedAt | formatDate }}</div>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"文件名\" prop=\"name\" width=\"180\" />\n        <el-table-column label=\"链接\" prop=\"url\" min-width=\"300\" />\n        <el-table-column label=\"标签\" prop=\"tag\" width=\"100\">\n          <template slot-scope=\"scope\">\n            <el-tag\n              :type=\"scope.row.tag === 'jpg' ? 'primary' : 'success'\"\n              disable-transitions\n            >{{ scope.row.tag }}</el-tag>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"操作\" width=\"160\">\n          <template slot-scope=\"scope\">\n            <el-button size=\"small\" type=\"text\" @click=\"downloadFile(scope.row)\">下载</el-button>\n            <el-button size=\"small\" type=\"text\" @click=\"deleteFile(scope.row)\">删除</el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n      <el-pagination\n        :current-page=\"page\"\n        :page-size=\"pageSize\"\n        :page-sizes=\"[10, 30, 50, 100]\"\n        :style=\"{ float: 'right', padding: '20px' }\"\n        :total=\"total\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        @current-change=\"handleCurrentChange\"\n        @size-change=\"handleSizeChange\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nconst path = process.env.VUE_APP_BASE_API\nimport { mapGetters } from 'vuex'\nimport infoList from '@/mixins/infoList'\nimport { getFileList, deleteFile } from '@/api/fileUploadAndDownload'\nimport { downloadImage } from '@/utils/downloadImg'\nimport { formatTimeToStr } from '@/utils/date'\nimport CustomPic from '@/components/customPic'\nimport UploadImage from '@/components/upload/image.vue'\nexport default {\n  name: 'Upload',\n  components: {\n    CustomPic,\n    UploadImage\n  },\n  filters: {\n    formatDate: function(time) {\n      if (time !== null && time !== '') {\n        var date = new Date(time)\n        return formatTimeToStr(date, 'yyyy-MM-dd hh:mm:ss')\n      } else {\n        return ''\n      }\n    }\n  },\n  mixins: [infoList],\n  data() {\n    return {\n      fullscreenLoading: false,\n      listApi: getFileList,\n      path: path,\n      tableData: [],\n      imageUrl: ''\n    }\n  },\n  computed: {\n    ...mapGetters('user', ['userInfo', 'token'])\n  },\n  created() {\n    this.getTableData()\n  },\n  methods: {\n    async deleteFile(row) {\n      this.$confirm('此操作将永久文件, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      })\n        .then(async() => {\n          const res = await deleteFile(row)\n          if (res.code === 0) {\n            this.$message({\n              type: 'success',\n              message: '删除成功!'\n            })\n            if (this.tableData.length === 1 && this.page > 1) {\n              this.page--\n            }\n            this.getTableData()\n          }\n        })\n        .catch(() => {\n          this.$message({\n            type: 'info',\n            message: '已取消删除'\n          })\n        })\n    },\n    checkFile(file) {\n      this.fullscreenLoading = true\n      const isJPG = file.type === 'image/jpeg'\n      const isPng = file.type === 'image/png'\n      const isLt2M = file.size / 1024 / 1024 < 2\n      if (!isJPG && !isPng) {\n        this.$message.error('上传头像图片只能是 JPG或png 格式!')\n        this.fullscreenLoading = false\n      }\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!')\n        this.fullscreenLoading = false\n      }\n      return (isPng || isJPG) && isLt2M\n    },\n    uploadSuccess(res) {\n      this.fullscreenLoading = false\n      if (res.code === 0) {\n        this.$message({\n          type: 'success',\n          message: '上传成功'\n        })\n        if (res.code === 0) {\n          this.getTableData()\n        }\n      } else {\n        this.$message({\n          type: 'warning',\n          message: res.msg\n        })\n      }\n    },\n    uploadError() {\n      this.$message({\n        type: 'error',\n        message: '上传失败'\n      })\n      this.fullscreenLoading = false\n    },\n    downloadFile(row) {\n      downloadImage(row.url, row.name)\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/view/example/upload"}]}