,OAAA,GAAA,IAAA,IAAA,GAAA,WAAA,EAAA;AACA,GArCA;AAsCA,EAAA,OAAA,kCACA,UAAA,CAAA,MAAA,EAAA,CAAA,SAAA,CAAA,CADA;AAEA,IAAA,SAFA,uBAEA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,OAAA,EADA;;AAAA;AACA,gBAAA,GADA;;AAEA,oBAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,mCAAA,GAAA,CAAA,IAAA,8CAAA,UAAA,QAAA,EAAA;AACA,oBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,eAAA;;AACA,oBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,sBAAA,IAAA,EAAA;AAAA,qBAAA;AACA,mBAHA,MAGA;AACA,oBAAA,KAAA,CAAA,QAAA,CAAA;AACA,sBAAA,IAAA,EAAA,MADA;AAEA,sBAAA,OAAA,EAAA;AAFA,qBAAA;AAIA;AACA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAfA;AAgBA,IAAA,KAhBA,mBAgBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,OAAA,CAAA,MAAA,CAAA,SAAA,CADA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,KAlBA;AAmBA,IAAA,UAnBA,wBAmBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,QAAA;AAAA,qFAAA,kBAAA,CAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCACA,CADA;AAAA;AAAA;AAAA;;AAAA;AAAA,mCAEA,MAAA,CAAA,KAAA,EAFA;;AAAA;AAEA,4BAAA,IAFA;;AAGA,gCAAA,CAAA,IAAA,EAAA;AACA,8BAAA,MAAA,CAAA,WAAA;AACA;;AALA;AAAA;;AAAA;AAOA,4BAAA,MAAA,CAAA,QAAA,CAAA;AACA,8BAAA,IAAA,EAAA,OADA;AAEA,8BAAA,OAAA,EAAA,WAFA;AAGA,8BAAA,SAAA,EAAA;AAHA,6BAAA;;AAKA,4BAAA,MAAA,CAAA,WAAA;;AAZA,8DAaA,KAbA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAAA;;AAAA;AAAA;AAAA;AAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA,KApCA;AAqCA,IAAA,UArCA,wBAqCA;AACA,WAAA,IAAA,GAAA,KAAA,IAAA,KAAA,MAAA,GAAA,QAAA,GAAA,MAAA;AACA,KAvCA;AAwCA,IAAA,WAxCA,yBAwCA;AAAA;;AACA,MAAA,OAAA,CAAA,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,SAAA;AACA,OAHA;AAIA;AA7CA;AAtCA,CAAA","sourcesContent":["<template>\n  <div id=\"userLayout\">\n    <div class=\"login_panle\">\n      <div class=\"login_panle_form\">\n        <div class=\"login_panle_form_title\">\n          <img class=\"login_panle_form_title_logo\" :src=\"$GIN_VUE_ADMIN.appLogo\" alt=\"\"><p class=\"login_panle_form_title_p\">{{ $GIN_VUE_ADMIN.appName }}</p>\n        </div>\n        <el-form\n          ref=\"loginForm\"\n          :model=\"loginForm\"\n          :rules=\"rules\"\n          @keyup.enter.native=\"submitForm\"\n        >\n          <el-form-item prop=\"username\">\n            <el-input v-model=\"loginForm.username\" placeholder=\"请输入用户名\">\n              <i slot=\"suffix\" class=\"el-input__icon el-icon-user\" />\n            </el-input>\n          </el-form-item>\n          <el-form-item prop=\"password\">\n            <el-input\n              v-model=\"loginForm.password\"\n              :type=\"lock === 'lock' ? 'password' : 'text'\"\n              placeholder=\"请输入密码\"\n            >\n              <i\n                slot=\"suffix\"\n                :class=\"'el-input__icon el-icon-' + lock\"\n                @click=\"changeLock\"\n              />\n            </el-input>\n          </el-form-item>\n          <el-form-item style=\"position: relative\">\n            <el-input\n              v-model=\"loginForm.captcha\"\n              name=\"logVerify\"\n              placeholder=\"请输入验证码\"\n              style=\"width: 60%\"\n            />\n            <div class=\"vPic\">\n              <img\n                v-if=\"picPath\"\n                :src=\"picPath\"\n                width=\"100%\"\n                height=\"100%\"\n                alt=\"请输入验证码\"\n                @click=\"loginVerify()\"\n              >\n            </div>\n          </el-form-item>\n          <div />\n          <el-form-item>\n            <el-button\n              type=\"primary\"\n              style=\"width: 46%\"\n              @click=\"checkInit\"\n            >前往初始化</el-button>\n            <el-button\n              type=\"primary\"\n              style=\"width: 46%;margin-left:8%\"\n              @click=\"submitForm\"\n            >登 录</el-button>\n          </el-form-item>\n        </el-form>\n      </div>\n      <div class=\"login_panle_right\" />\n      <div class=\"login_panle_foot\">\n        <div class=\"links\">\n          <a href=\"http://doc.henrongyi.top/\"><img src=\"@/assets/docs.png\" class=\"link-icon\"></a>\n          <a href=\"https://www.yuque.com/flipped-aurora/\"><img src=\"@/assets/yuque.png\" class=\"link-icon\"></a>\n          <a href=\"https://github.com/flipped-aurora/gin-vue-admin\"><img src=\"@/assets/github.png\" class=\"link-icon\"></a>\n          <a href=\"https://space.bilibili.com/322210472\"><img src=\"@/assets/video.png\" class=\"link-icon\"></a>\n        </div>\n        <div class=\"copyright\">Copyright &copy; {{ curYear }} 💖 flipped-aurora</div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex'\nimport { captcha } from '@/api/user'\nimport { checkDB } from '@/api/initdb'\nexport default {\n  name: 'Login',\n  data() {\n    const checkUsername = (rule, value, callback) => {\n      if (value.length < 3) {\n        return callback(new Error('请输入正确的用户名'))\n      } else {\n        callback()\n      }\n    }\n    const checkPassword = (rule, value, callback) => {\n      if (value.length < 6) {\n        return callback(new Error('请输入正确的密码'))\n      } else {\n        callback()\n      }\n    }\n    return {\n      curYear: 0,\n      lock: 'lock',\n      loginForm: {\n        username: 'admin',\n        password: '123456',\n        captcha: '',\n        captchaId: ''\n      },\n      rules: {\n        username: [{ validator: checkUsername, trigger: 'blur' }],\n        password: [{ validator: checkPassword, trigger: 'blur' }]\n      },\n      logVerify: '',\n      picPath: ''\n    }\n  },\n  created() {\n    this.loginVerify()\n    this.curYear = new Date().getFullYear()\n  },\n  methods: {\n    ...mapActions('user', ['LoginIn']),\n    async checkInit() {\n      const res = await checkDB()\n      if (res.code === 0) {\n        if (res.data?.needInit) {\n          this.$store.commit('user/NeedInit')\n          this.$router.push({ name: 'Init' })\n        } else {\n          this.$message({\n            type: 'info',\n            message: '已配置数据库信息，无法初始化'\n          })\n        }\n      }\n    },\n    async login() {\n      return await this.LoginIn(this.loginForm)\n    },\n    async submitForm() {\n      this.$refs.loginForm.validate(async(v) => {\n        if (v) {\n          const flag = await this.login()\n          if (!flag) {\n            this.loginVerify()\n          }\n        } else {\n          this.$message({\n            type: 'error',\n            message: '请正确填写登录信息',\n            showClose: true\n          })\n          this.loginVerify()\n          return false\n        }\n      })\n    },\n    changeLock() {\n      this.lock = this.lock === 'lock' ? 'unlock' : 'lock'\n    },\n    loginVerify() {\n      captcha({}).then((ele) => {\n        this.picPath = ele.data.picPath\n        this.loginForm.captchaId = ele.data.captchaId\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"~@/style/newLogin.scss\";\n</style>\n"],"sourceRoot":"src/view/login"}]}