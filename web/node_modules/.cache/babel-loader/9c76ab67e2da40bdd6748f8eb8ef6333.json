GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;AACA,kBAAA,KAAA,CAAA,iBAAA,GAAA,IAAA;AACA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KARA;AASA,IAAA,WATA,yBASA;AACA,WAAA,iBAAA,GAAA,KAAA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,YAAA,EAAA,EADA;AAEA,QAAA,iBAAA,EAAA;AAFA,OAAA;AAIA,KAfA;AAgBA,IAAA,cAhBA,0BAgBA,GAhBA,EAgBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,GAAA,CAAA,OAAA,GAAA,KAAA;AADA;AAAA,uBAEA,iBAAA,CAAA;AAAA,kBAAA,EAAA,EAAA,GAAA,CAAA;AAAA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;;AAGA,oBAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA;AACA,oBAAA,IAAA,EAAA,SADA;AAEA,oBAAA,OAAA,EAAA;AAFA,mBAAA;;AAIA,sBAAA,MAAA,CAAA,SAAA,CAAA,MAAA,KAAA,CAAA,IAAA,MAAA,CAAA,IAAA,GAAA,CAAA,EAAA;AACA,oBAAA,MAAA,CAAA,IAAA;AACA;;AACA,kBAAA,MAAA,CAAA,YAAA;AACA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KA7BA;AA8BA,IAAA,WA9BA,yBA8BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+BAEA,MAAA,CAAA,IAFA;AAAA,kDAGA,QAHA,wBAMA,QANA;AAAA;;AAAA;AAAA;AAAA,uBAIA,iBAAA,CAAA,MAAA,CAAA,IAAA,CAJA;;AAAA;AAIA,gBAAA,GAJA;AAAA;;AAAA;AAAA;AAAA,uBAOA,iBAAA,CAAA,MAAA,CAAA,IAAA,CAPA;;AAAA;AAOA,gBAAA,GAPA;AAAA;;AAAA;AAAA;AAAA,uBAUA,iBAAA,CAAA,MAAA,CAAA,IAAA,CAVA;;AAAA;AAUA,gBAAA,GAVA;AAAA;;AAAA;AAcA,oBAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,WAAA;;AACA,kBAAA,MAAA,CAAA,YAAA;AACA;;AAjBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KAhDA;AAiDA,IAAA,UAjDA,wBAiDA;AACA,WAAA,IAAA,GAAA,QAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA;AApDA;AA3BA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"search-term\">\n      <el-form :inline=\"true\" :model=\"searchInfo\" class=\"demo-form-inline\">\n        <el-form-item>\n          <el-button size=\"mini\" type=\"primary\" icon=\"el-icon-search\" @click=\"openDialog\">新增</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n    <el-table\n      ref=\"multipleTable\"\n      :data=\"tableData\"\n      border\n      stripe\n      style=\"width: 100%\"\n      tooltip-effect=\"dark\"\n    >\n      <el-table-column type=\"selection\" width=\"55\" />\n      <el-table-column label=\"接入日期\" width=\"180\">\n        <template slot-scope=\"scope\">{{ scope.row.CreatedAt|formatDate }}</template>\n      </el-table-column>\n      <el-table-column label=\"姓名\" prop=\"customerName\" width=\"120\" />\n      <el-table-column label=\"电话\" prop=\"customerPhoneData\" width=\"120\" />\n      <el-table-column label=\"接入人ID\" prop=\"sysUserId\" width=\"120\" />\n      <el-table-column label=\"按钮组\" min-width=\"160\">\n        <template slot-scope=\"scope\">\n          <el-button size=\"small\" type=\"text\" @click=\"updateCustomer(scope.row)\">变更</el-button>\n          <el-popover v-model=\"scope.row.visible\" placement=\"top\" width=\"160\">\n            <p>确定要删除吗？</p>\n            <div style=\"text-align: right; margin: 0\">\n              <el-button size=\"mini\" type=\"text\" @click=\"scope.row.visible = false\">取消</el-button>\n              <el-button type=\"primary\" size=\"mini\" @click=\"deleteCustomer(scope.row)\">确定</el-button>\n            </div>\n            <el-button slot=\"reference\" type=\"danger\" icon=\"el-icon-delete\" size=\"mini\">删除</el-button>\n          </el-popover>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <el-pagination\n      :current-page=\"page\"\n      :page-size=\"pageSize\"\n      :page-sizes=\"[10, 30, 50, 100]\"\n      :style=\"{float:'right',padding:'20px'}\"\n      :total=\"total\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      @current-change=\"handleCurrentChange\"\n      @size-change=\"handleSizeChange\"\n    />\n\n    <el-dialog :before-close=\"closeDialog\" :visible.sync=\"dialogFormVisible\" title=\"客户\">\n      <el-form :inline=\"true\" :model=\"form\" label-width=\"80px\">\n        <el-form-item label=\"客户名\">\n          <el-input v-model=\"form.customerName\" autocomplete=\"off\" />\n        </el-form-item>\n        <el-form-item label=\"客户电话\">\n          <el-input v-model=\"form.customerPhoneData\" autocomplete=\"off\" />\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"closeDialog\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"enterDialog\">确 定</el-button>\n      </div>\n    </el-dialog>\n    <div class=\"tips\">在资源权限中将此角色的资源权限清空 或者不包含创建者的角色 即可屏蔽此客户资源的显示</div>\n  </div>\n</template>\n\n<script>\nimport {\n  createExaCustomer,\n  updateExaCustomer,\n  deleteExaCustomer,\n  getExaCustomer,\n  getExaCustomerList\n} from '@/api/customer'\nimport { formatTimeToStr } from '@/utils/date'\nimport infoList from '@/mixins/infoList'\n\nexport default {\n  name: 'Customer',\n  filters: {\n    formatDate: function(time) {\n      if (time !== null && time !== '') {\n        var date = new Date(time)\n        return formatTimeToStr(date, 'yyyy-MM-dd hh:mm:ss')\n      } else {\n        return ''\n      }\n    }\n  },\n  mixins: [infoList],\n  data() {\n    return {\n      listApi: getExaCustomerList,\n      dialogFormVisible: false,\n      type: '',\n      form: {\n        customerName: '',\n        customerPhoneData: ''\n      }\n    }\n  },\n  created() {\n    this.getTableData()\n  },\n  methods: {\n    async updateCustomer(row) {\n      const res = await getExaCustomer({ ID: row.ID })\n      this.type = 'update'\n      if (res.code === 0) {\n        this.form = res.data.customer\n        this.dialogFormVisible = true\n      }\n    },\n    closeDialog() {\n      this.dialogFormVisible = false\n      this.form = {\n        customerName: '',\n        customerPhoneData: ''\n      }\n    },\n    async deleteCustomer(row) {\n      row.visible = false\n      const res = await deleteExaCustomer({ ID: row.ID })\n      if (res.code === 0) {\n        this.$message({\n          type: 'success',\n          message: ''\n        })\n        if (this.tableData.length === 1 && this.page > 1) {\n          this.page--\n        }\n        this.getTableData()\n      }\n    },\n    async enterDialog() {\n      let res\n      switch (this.type) {\n        case 'create':\n          res = await createExaCustomer(this.form)\n          break\n        case 'update':\n          res = await updateExaCustomer(this.form)\n          break\n        default:\n          res = await createExaCustomer(this.form)\n          break\n      }\n\n      if (res.code === 0) {\n        this.closeDialog()\n        this.getTableData()\n      }\n    },\n    openDialog() {\n      this.type = 'create'\n      this.dialogFormVisible = true\n    }\n  }\n}\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src/view/example/customer"}]}